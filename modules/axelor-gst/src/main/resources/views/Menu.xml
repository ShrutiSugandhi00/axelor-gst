<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">


	<!-- Party Menu -->

	<menuitem title="Party" name="party-root" />
	<menuitem title="Party" name="party-root-party-child" parent="party-root"
		action="action.gst.view.party" />
	<action-view name="action.gst.view.party" title="Party"
		model="com.axelor.gst.db.Party">
		<view type="grid" name="party-grid" />
		<view type="form" name="party-form" />
		<view type="cards" name="party-card" />
	</action-view>
	<menuitem title="Contact" name="party-root-contact-child"
		parent="party-root" action="action.gst.view.contact" />
	<action-view name="action.gst.view.contact" title="Contact"
		model="com.axelor.gst.db.Contact">
		<view type="grid" name="contact-grid" />
		<view type="form" name="contact-form" />
	</action-view>
	<menuitem title="Configurations" name="party-root-configuration-child"
		parent="party-root" />

	<menuitem title="Address" name="configuration-root-address-child"
		parent="party-root-configuration-child" action="action.gst.view.address" />
	<action-view name="action.gst.view.address" title="Address"
		model="com.axelor.gst.db.Address">
		<view type="grid" name="address-grid" />
		<view type="form" name="address-form" />
	</action-view>
	<menuitem title="City" name="configuration-root-city-child"
		parent="party-root-configuration-child" action="action.gst.view.city" />
	<action-view name="action.gst.view.city" title="City"
		model="com.axelor.gst.db.City">
		<view type="grid" name="city-grid" />
		<view type="form" name="city-form" />
	</action-view>
	<menuitem title="State" name="configuration-root-state-child"
		parent="party-root-configuration-child" action="action.gst.view.state" />
	<action-view name="action.gst.view.state" title="State"
		model="com.axelor.gst.db.State">
		<view type="grid" name="state-grid" />
		<view type="form" name="state-form" />
	</action-view>
	<menuitem title="Country" name="configuration-root-country-child"
		parent="party-root-configuration-child" action="action.gst.view.country" />
	<action-view name="action.gst.view.country" title="Country"
		model="com.axelor.gst.db.Country">
		<view type="grid" name="country-grid" />
		<view type="form" name="country-form" />
	</action-view>

	<!-- Product menu -->


	<menuitem title="Product" name="product-root" />
	<menuitem title="Product" name="produt-root-product-child"
		parent="product-root" action="action.gst.view.product" />
	<action-view name="action.gst.view.product" title="Product"
		model="com.axelor.gst.db.Product">
		<view type="grid" name="product-grid" />
		<view type="form" name="product-form" />
	</action-view>

	<menuitem title="Product Categories" name="product-root-product-category-child"
		parent="product-root" action="action.gst.view.product.category" />
	<action-view name="action.gst.view.product.category" title="Product Categories"
		model="com.axelor.gst.db.ProductCategory">
		<view type="grid" name="productCategory-grid" />
		<view type="form" name="productCategory-form" />
	</action-view>

	<!-- Invoice Menu -->

	<menuitem title="Invoice" name="invoice-root" />
	<menuitem title="Create Invoice" name="invoice-root-create-invoice-child"
		parent="invoice-root" action="action.gst.view.invoice" />
	<action-view name="action.gst.view.invoice" title="Invoice"
		model="com.axelor.gst.db.Invoice">
		<view type="form" name="invoice-form" />
		<view type="grid" name="invoice-grid" />
		<view type="kanban" name="invoice-kanban" />
	</action-view>


	<!-- Company Menu -->
	<menuitem title="Company" name="comapany-root" />
	<menuitem title="Company" name="comapany-root-company-child"
		parent="comapany-root" action="action.gst.view.company" />
	<action-view name="action.gst.view.company" title="Company"
		model="com.axelor.gst.db.Company">
		<view type="grid" name="company-grid" />
		<view type="form" name="company-form" />

	</action-view>
	<menuitem title="Bank Account" name="comapany-root-bank-account-child"
		parent="comapany-root" action="action.gst.view.bank.account" />
	<action-view name="action.gst.view.bank.account" title="Bank Account"
		model="com.axelor.gst.db.BankAccount">
		<view type="grid" name="bankAccount-grid" />
		<view type="form" name="bankAccount-form" />
	</action-view>
	<menuitem title="Sequence" name="comapany-root-sequence-child"
		parent="comapany-root" action="action.gst.view.sequence" />
	<action-view name="action.gst.view.sequence" title="Sequence"
		model="com.axelor.gst.db.Sequence">
		<view type="grid" name="sequence-grid" />
		<view type="form" name="sequence-form" />
	</action-view>


</object-views>
